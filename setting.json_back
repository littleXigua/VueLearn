{
    "git.ignoreMissingGitWarning": true,
    "editor.fontSize": 16,
    "workbench.iconTheme": "vscode-icons",
    "vetur.format.defaultFormatterOptions": {
    
        "js-beautify-html": {
            "wrap_attributes": "force-expand-multiline"
        },
        "prettyhtml": {
            "printWidth": 100,
            "singleQuote": false,
            "wrapAttributes": false,
            "sortAttributes": false
        }
    }
}